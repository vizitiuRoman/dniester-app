{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "isready": "npm run prettier && npm run lint && npm run ts-coverage",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "ts-coverage": "typescript-coverage-report",
    "compile": "protoc -I ../../ --js_out=import_style=commonjs:src/grpc --grpc-web_out=import_style=commonjs+dts,mode=grpcwebtext:src/grpc ../../grpc-proto/**/*.proto",
    "lint": "tslint -p tsconfig.json"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.4",
    "axios": "^0.21.1",
    "grpc-web": "^1.2.1",
    "i18next": "^20.2.2",
    "react": "17.0.1",
    "react-i18next": "^11.8.14",
    "react-native": "0.64.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.1.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.1.1",
    "rxjs": "^6.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/google-protobuf": "^3.15.2",
    "@types/jest": "^26.0.20",
    "@types/react-native": "^0.64.0",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "codelyzer": "^6.0.1",
    "eslint": "^7.14.0",
    "google-protobuf": "^3.15.8",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^2.2.1",
    "protoc-gen-grpc-web": "^1.2.1",
    "react-test-renderer": "17.0.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^3.8.3",
    "typescript-coverage-report": "^0.6.0"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
